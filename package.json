{
  "name": "redux-serial-effects",
  "version": "0.0.6",
  "description": "Predictable side-effect middleware for the predictable state container",
  "license": "MIT",
  "main": "dist/index.js",
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --cache --format codeframe --ext .js"
      ]
    }
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "babel src --ignore '__tests__' --out-dir dist",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint --cache --format codeframe --ext .js .",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "release": "wnpm-release --no-shrinkwrap",
    "test": "jest --coverage",
    "test:debug": "jest --runInBand",
    "watch": "jest --watch"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix/redux-serial-effects.git"
  },
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "transformIgnorePatterns": [
      "^.*\\.js$"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/.history/"
    ],
    "testURL": "http://localhost"
  },
  "dependencies": {
    "@aantron/repromise": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.6.1",
    "bs-platform": "^4.0.18",
    "eslint": "^3.16.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-teamcity-reporter": "0.9.0",
    "lint-staged": "^5.0.0",
    "prettier": "^1.7.0",
    "redux": "^3.6.0",
    "rimraf": "^2.6.2",
    "wnpm-ci": "*"
  }
}
